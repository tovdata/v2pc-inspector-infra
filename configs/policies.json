[{
  "document": {
    "Version": "2012-10-17",
    "Statement": [{
      "Effect": "Allow",
      "Action": ["dynamodb:PutItem", "dynamodb:GetItem", "dynamodb:Query", "dynamodb:UpdateItem"],
      "Resource": "*"
    },{
      "Effect": "Allow",
      "Action": "ec2:DescribeRegions",
      "Resource": "*"
    }]
  },
  "name": "v2pc-inspector-to-read-write-dynamodb-items"
}, {
  "document": {
    "Version": "2012-10-17",
    "Statement": [{
      "Effect": "Allow",
      "Action": "sts:AssumeRole",
      "Resource": "*"
    }]
  },
  "name": "v2pc-inspector-to-assume-role"
}, {
  "document": {
    "Version": "2012-10-17",
    "Statement": [{
      "Effect": "Allow",
      "Action": ["s3:PutObject", "s3:GetObject", "s3:DeleteObject"],
      "Resource": "*"
    }]
  },
  "name": "v2pc-inspector-to-access-s3-object"
}, {
  "document": {
    "Version": "2012-10-17",
    "Statement": [{
      "Effect": "Allow",
      "Action": ["sqs:DeleteMessage", "sqs:ReceiveMessage", "sqs:SendMessage"],
      "Resource": "*"
    }]
  },
  "name": "v2pc-inspector-to-read-write-sqs-message"
}]